    "color_palette": {
      "primary": "#2D2D2D",
      "secondary": "#FFFFFF",
      "accent": "#0066FF",
      "background": "#F5F5F5",
      "text": "#333333"
    },
    "typography": {
      "heading": {
        "font": "Inter",
        "weight": 500,
        "letterSpacing": "-0.02em",
